<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.docmall.mapper.CommonCodeMapper">


	<!-- 자식코드로 자식테이블 셀렉트 -->
	<select id="commonCodeChildSel" parameterType="long" resultType="com.docmall.domain.CommonCodeVO">
		SELECT COMMON_CODE_CHILD, 
			   COMMON_CODE, 
			   COMMON_CODE_NM
		FROM COMMON_CODE_DETAIL_TBL 
		WHERE COMMON_CODE_CHILD = #{common_code}
	</select>
</mapper>